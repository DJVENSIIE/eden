<BorderPane prefHeight="600.0" prefWidth="900.0" styleClass="app-background" stylesheets="@../style/style.css"
            xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1"
   fx:controller="com.lgs.eden.views.marketplace.Marketplace">
   <top>
      <VBox alignment="CENTER" BorderPane.alignment="CENTER">
         <BorderPane.margin>
            <Insets top="10.0" />
         </BorderPane.margin>
         <children>
            <Label styleClass="yellow-text" text="Library">
               <font>
                  <Font name="System Bold" size="36.0" />
               </font>
            </Label>
            <Label layoutX="351.0" layoutY="10.0" styleClass="yellow-text" text="Feel free to request a game using our website contact form, we might create it for your!">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
            </Label>
         </children>
      </VBox>
   </top>
   <center>
      <GridPane hgap="20.0" styleClass="app-background" vgap="20.0" BorderPane.alignment="CENTER"
      fx:id="content">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <BorderPane.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </BorderPane.margin>
         <children>
         </children>
      </GridPane>
   </center>
   <bottom>
      <Pagination maxPageIndicatorCount="1" fx:id="paginations" pageCount="1" BorderPane.alignment="CENTER" />
   </bottom>
</BorderPane>
